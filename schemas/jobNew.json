{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["title"],
  "properties": {
    "title": {
      "type": "string",
      "default": "",
      "title": "The title Schema",
      "examples": ["driver"]
    },
    "companyHandle": {
      "type": "string",
      "default": "",
      "title": "The companyHandle Schema",
      "examples": ["Bay Area LightWorks"]
    },
    "salary": {
      "type": "integer",
      "default": 0,
      "title": "The salary Schema",
      "examples": [50000]
    },
    "equity": {
      "type": "integer",
      "default": 0,
      "title": "The equity Schema",
      "examples": [0]
    }
  },
  "examples": [
    {
      "title": "driver",
      "companyHandle": "Bay Area LightWorks",
      "salary": 50000,
      "equity": 0
    }
  ]
}
